#: kdeconnect-cli.cpp:44
#, kde-format
msgid "KDE Connect CLI tool"
msgstr "Eina CLI per a KDE Connect"

#: kdeconnect-cli.cpp:46
#, kde-format
msgid "(C) 2015 Aleix Pol Gonzalez"
msgstr "(C) 2015 Aleix Pol Gonzalez"

#: kdeconnect-cli.cpp:49
#, kde-format
msgid "Aleix Pol Gonzalez"
msgstr "Aleix Pol Gonzalez"

#: kdeconnect-cli.cpp:50
#, kde-format
msgid "Albert Vaca Cintora"
msgstr "Albert Vaca Cintora"

#: kdeconnect-cli.cpp:52
#, kde-format
msgid "List all devices"
msgstr "Llista tots els dispositius"

#: kdeconnect-cli.cpp:53
#, kde-format
msgid "List available (paired and reachable) devices"
msgstr "Llista tots els dispositius disponibles (aparellats i accessibles)"

#: kdeconnect-cli.cpp:54
#, kde-format
msgid ""
"Make --list-devices or --list-available print only the devices id, to ease "
"scripting"
msgstr ""
"Fa que --list-devices o --list-available imprimisca només els ID dels "
"dispositius, per a facilitar la creació de scripts"

#: kdeconnect-cli.cpp:55
#, kde-format
msgid ""
"Make --list-devices or --list-available print only the devices name, to ease "
"scripting"
msgstr ""
"Fa que --list-devices o --list-available imprimisca només els noms dels "
"dispositius, per a facilitar la creació de scripts"

#: kdeconnect-cli.cpp:56
#, kde-format
msgid ""
"Make --list-devices or --list-available print only the devices id and name, "
"to ease scripting"
msgstr ""
"Fa que --list-devices o --list-available imprimisca només els ID i els noms "
"dels dispositius, per a facilitar la creació de scripts"

#: kdeconnect-cli.cpp:57
#, kde-format
msgid "Search for devices in the network and re-establish connections"
msgstr "Busca dispositius en la xarxa i torna a establir les connexions"

#: kdeconnect-cli.cpp:58
#, kde-format
msgid "Request pairing to a said device"
msgstr "Demana l'aparellament amb un dispositiu indicat"

#: kdeconnect-cli.cpp:59
#, kde-format
msgid "Find the said device by ringing it."
msgstr "Troba el dispositiu indicat cridant-lo."

#: kdeconnect-cli.cpp:60
#, kde-format
msgid "Stop pairing to a said device"
msgstr "Para l'aparellament amb un dispositiu indicat"

#: kdeconnect-cli.cpp:61
#, kde-format
msgid "Sends a ping to said device"
msgstr "Envia un PING a un dispositiu indicat"

#: kdeconnect-cli.cpp:62
#, kde-format
msgid "Same as ping but you can set the message to display"
msgstr "Igual que el PING però es pot definir el missatge que es mostrarà"

#: kdeconnect-cli.cpp:62 kdeconnect-cli.cpp:67
#, kde-format
msgid "message"
msgstr "missatge"

#: kdeconnect-cli.cpp:63
#, kde-format
msgid "Share a file to a said device"
msgstr "Comparteix un fitxer amb un dispositiu indicat"

#: kdeconnect-cli.cpp:64
#, kde-format
msgid "Share text to a said device"
msgstr "Comparteix text amb un dispositiu indicat"

#: kdeconnect-cli.cpp:65
#, kde-format
msgid "Display the notifications on a said device"
msgstr "Mostra les notificacions en un dispositiu indicat"

#: kdeconnect-cli.cpp:66
#, kde-format
msgid "Lock the specified device"
msgstr "Bloqueja el dispositiu especificat"

#: kdeconnect-cli.cpp:67
#, kde-format
msgid "Sends an SMS. Requires destination"
msgstr "Envia un SMS. Requereix una destinació"

#: kdeconnect-cli.cpp:68
#, kde-format
msgid "Phone number to send the message"
msgstr "Número de telèfon on enviar el missatge"

#: kdeconnect-cli.cpp:68
#, kde-format
msgid "phone number"
msgstr "número de telèfon"

#: kdeconnect-cli.cpp:69
#, kde-format
msgid "Device ID"
msgstr "ID del dispositiu"

#: kdeconnect-cli.cpp:70
#, kde-format
msgid "Device Name"
msgstr "Nom del dispositiu"

#: kdeconnect-cli.cpp:71
#, kde-format
msgid "Get encryption info about said device"
msgstr "Obtín la informació d'encriptació del dispositiu indicat"

#: kdeconnect-cli.cpp:72
#, kde-format
msgid "Lists remote commands and their ids"
msgstr "Llista les ordres remotes i els seus ID"

#: kdeconnect-cli.cpp:73
#, kde-format
msgid "Executes a remote command by id"
msgstr "Executa una ordre remota pel seu ID"

#: kdeconnect-cli.cpp:74
#, kde-format
msgid "Sends keys to a said device"
msgstr "Envia les tecles a un dispositiu indicat"

#: kdeconnect-cli.cpp:75
#, kde-format
msgid "Display this device's id and exit"
msgstr "Mostra l'ID d'aquest dispositiu i ix"

#: kdeconnect-cli.cpp:76
#, kde-format
msgid "Open the connected device's camera and transfer the photo"
msgstr "Obri la càmera del dispositiu connectat i transfereix la fotografia"

#: kdeconnect-cli.cpp:124
#, kde-format
msgid "(paired and reachable)"
msgstr "(aparellat i accessible)"

#: kdeconnect-cli.cpp:126
#, kde-format
msgid "(reachable)"
msgstr "(accessible)"

#: kdeconnect-cli.cpp:128 kdeconnect-cli.cpp:149
#, kde-format
msgid "(paired)"
msgstr "(aparellat)"

#: kdeconnect-cli.cpp:135
#, kde-format
msgid "1 device found"
msgid_plural "%1 devices found"
msgstr[0] "S'ha trobat 1 dispositiu"
msgstr[1] "S'han trobat %1 dispositius"

#: kdeconnect-cli.cpp:137
#, kde-format
msgid "No devices found"
msgstr "No s'ha trobat cap dispositiu"

#: kdeconnect-cli.cpp:151
#, kde-format
msgid "(unpaired)"
msgstr "(sense aparellar)"

#: kdeconnect-cli.cpp:185
#, kde-format
msgid "No device specified"
msgstr "No s'ha especificat cap dispositiu"

#: kdeconnect-cli.cpp:214
#, kde-format
msgid "Shared %1"
msgstr "Compartit %1"

#: kdeconnect-cli.cpp:220
#, kde-format
msgid "Shared text: %1"
msgstr "Text compartit %1"

#: kdeconnect-cli.cpp:226
#, kde-format
msgid "waiting for device..."
msgstr "s'està esperant un dispositiu..."

#: kdeconnect-cli.cpp:240
#, kde-format
msgid "Device not found"
msgstr "No s'ha trobat cap dispositiu"

#: kdeconnect-cli.cpp:242
#, kde-format
msgid "Already paired"
msgstr "Ja està aparellat"

#: kdeconnect-cli.cpp:244
#, kde-format
msgid "Pair requested"
msgstr "Sol·licitud per a aparellar"

#: kdeconnect-cli.cpp:251
#, kde-format
msgid "Already not paired"
msgstr "Ja no està aparellat"

#: kdeconnect-cli.cpp:253
#, kde-format
msgid "Unpaired"
msgstr "Sense aparellar"

#: kdeconnect-cli.cpp:269
#, kde-format
msgid ""
"error: should specify the SMS's recipient by passing --destination <phone "
"number>"
msgstr ""
"Error: cal especificar el destinatari de l'SMS informant --destination "
"<número de telèfon>"

#: kdeconnect-cli.cpp:282
#, kde-format
msgid "Please specify a filename for the photo"
msgstr "Especifiqueu un nom de fitxer per a la foto"

#: kdeconnect-cli.cpp:325
#, kde-format
msgid "Nothing to be done"
msgstr "No hi ha res que calga fer"
